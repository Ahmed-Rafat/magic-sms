{
    "name": "ahmedrafat/magic-sms",
    "description": "Magic SMS is a Package for an integration with any sms gatway",
    "keywords": [
        "ahmedrafat",
        "magic-sms"
    ],
    "homepage": "https://github.com/Ahmed-Rafat/magic-sms",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "AhmEd RaFat",
            "email": "ahmed.rafat147@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7",
        "illuminate/support": "5.6.x|5.7.x|5.8.x|6.x.x|7.x.x|8.x.x"
    },
    "autoload": {
        "psr-4": {
            "AR\\MagicSms\\": "src"
        },
        "files": [
            "src/Helpers/heplers.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AR\\MagicSms\\Providers\\MagicSmsServiceProvider"
            ],
            "aliases": {
                "MagicSms": "AR\\MagicSms\\MagicSms"
            }
        }
    }
}
